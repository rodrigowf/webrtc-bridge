PORT=8080
OPENAI_API_KEY=sk-REPLACE_ME
REALTIME_MODEL=gpt-realtime

# SSL Configuration (HTTPS is enabled by default for mobile WebRTC support)
# Set to 'false' to disable HTTPS and use HTTP only
SSL_ENABLED=true

# Optional: Provide your own SSL certificates (e.g., from mkcert or Let's Encrypt)
# If not provided, self-signed certs will be auto-generated in ~/.vcode/ssl/
# SSL_CERT_PATH=/path/to/cert.pem
# SSL_KEY_PATH=/path/to/key.pem
